{
  "compilerOptions": {
    "jsx": "react",
    "target": "es2016",
    "noImplicitAny": true,
    "strict": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    /* Modules */
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@e-comerce/*": ["src/*"],
      "@e-comerce/": ["src/"]
    }
  }
}
